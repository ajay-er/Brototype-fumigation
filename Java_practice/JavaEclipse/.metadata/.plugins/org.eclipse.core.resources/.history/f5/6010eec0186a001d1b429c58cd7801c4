package prac11;

public class Hi {
	
		public  int x=7777;
		public void method()
		{
			System.out.println("Ajay");
		}
}

class Koo extends Hi{
		public  int x=5555;
		public void method()
		{
			System.out.println("Second....");
			System.out.println(super.x);
		}
		
}


class Hello{
	public static void main(String[] args) {
		
			Koo s=new Koo();
			System.out.println(s.x);
			s.method();
			
		
	}
}
