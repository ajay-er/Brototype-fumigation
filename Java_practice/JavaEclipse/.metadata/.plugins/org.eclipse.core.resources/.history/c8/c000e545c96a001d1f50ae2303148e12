package GetSet;

public class Set {
		private String cash;

		public String getCash() {
			return cash;
		}

		public void setCash(String cash) {
			this.cash = cash;
		}
}

class Main{
	
	public static void main(String[] args) {
		Set d=new Set();
		d.getCash();
		d.setCash("No money No Pani");
		System.out.println(d.getCash());
	}
	
}
