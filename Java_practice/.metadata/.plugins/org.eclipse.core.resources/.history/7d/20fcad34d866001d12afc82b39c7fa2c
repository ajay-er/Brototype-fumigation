

  class C {

		void data()
		{
			System.out.println("first constroctor");
		}
}
  
  class B extends C
  {
	  
  }
  
class D extends C
{
		void disp()
		{
			System.out.println("second constructor");
		}
}
public class A {
	
	public static void main(String[] args) {
		D obj=new D();
		obj.disp();
		
	}
}