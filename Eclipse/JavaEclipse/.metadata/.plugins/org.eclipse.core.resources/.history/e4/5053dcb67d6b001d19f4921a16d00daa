package prac20;

public class A {

	A()
	{
		this(3);
		System.out.println("1st");
	}
	
	A(int x)
	{	
		this(3.3f);
		System.out.println("Second");
	}
	
	A(float x)
	{
		System.out.println("Third");
	}
	
	public static void main(String[] args) {
		A s=new A();
		B d=new B();
	}

}

class B extends A {
		
			B()
			{
				this(3,3);
				System.out.println();
				System.out.println("B1");
			}
			B(int d,int x)
			{
				System.out.println("B2");

			}
			B(int s)
			{
				System.out.println("B3");

			}
	
	
}






