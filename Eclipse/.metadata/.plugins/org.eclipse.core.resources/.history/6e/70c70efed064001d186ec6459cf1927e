
public class Main {
	
	public static void pattern1()
	{
		int i,j,n=4,p=1;
	    for(i=1;i<=n;i++)
	    {
	        
	        for(j=1;j<=i;j++)
	        {  
	        	System.out.print(p);
	        }
	        System.out.println();
	        
	    }
	}
	

	public static void main(String []args) {
		
		Main obj=new Main();
		obj.pattern1();
		
	}

}
